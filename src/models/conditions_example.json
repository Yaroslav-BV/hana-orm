## Operators

-EQ =
-NE !=
-GE >=
-LE <=
-GT >
-LT <

## Examples

### Input

base: {
  key: 'key',
  operator: 'EQ',
  value: 'value' 
}

where: {
  base 
}

### Output

WHERE key = 'value'

### Input

base1: {
  key: 'key1',
  operator: 'EQ',
  value: 'value1' 
}

base2: {
  key: 'key2',
  operator: 'EQ',
  value: 'value2' 
}

where: {
  or: [
    base1,
    base2
  ]
}

### Output

WHERE key1 = 'value1' OR key2 = 'value2'

### Input

base1: {
  key: 'key1',
  operator: 'EQ',
  value: 'value1' 
}

base2: {
  key: 'key2',
  operator: 'EQ',
  value: 'value2' 
}

base3: {
  key: 'key3',
  operator: 'GT',
  value: 'value3' 
}

where: {
  and: [
    or: [
      base1,
      base2
    ],
    base3
  ]
}

### Output

WHERE (key1 = 'value1' OR key2 = 'value2') AND key3 >= 'value3'